{
    "name": "NMD WP React Theme",
    "description": "NMD WP React Theme",
    "type": "project",
    "authors": [
        {
            "name": "Paradowski",
            "email": "me@nickmantia.design"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "^1.3.0",
        "vlucas/phpdotenv": "^2.4.0",
        "johnpbloch/wordpress": "^5.1",
				"wpackagist-plugin/jwt-authentication-for-wp-rest-api": "^1.2.6"
    },
    "extra": {
        "installer-paths": {
            "wordpress/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "wordpress/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "wordpress/wp"
    },
    "config": {
        "secure-http": false
    }
}
