{
	"name": "react-headless",
	"version": "1.0.0",
	"description": ":fire: Front end React application.",
	"main": "index.js",
	"scripts": {
		"webpack-dev-server": "webpack-dev-server",
		"dev": "webpack-dev-server --mode=development",
		"prod": "webpack --mode=production",
		"eslint": "eslint ./src/*.js ./src/**/**/**/*.js ./src/**/**/**/**/*.js",
		"eslint:fix": "eslint --fix ./src/*.js ./src/**/**/**/*.js ./src/**/**/**/**/*.js",
		"stylelint": "stylelint ./src/*.scss",
		"stylelint:fix": "stylelint ./src/*.scss --fix",
		"format-js": "prettier .prettierrc.js --write ./src/*.js ./src/**/**/**/*.js ./src/**/**/**/**/*.js",
		"precommit": "npm run format-js && npm run eslint:fix && npm run stylelint:fix && npm run test",
		"test": "jest --verbose --passWithNoTests",
		"test:debug": "jest --runInBand",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage=true",
		"test:update": "jest --verbose --updateSnapshot"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nmantia12/react-wordpress.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/nmantia12/react-wordpress/issues"
	},
	"homepage": "https://github.com/nmantia12/react-wordpress#readme",
	"dependencies": {
		"@reach/router": "^1.2.1",
		"axios": "^0.18.1",
		"dat.gui": "^0.7.7",
		"moment": "^2.24.0",
		"qs": "^6.7.0",
		"querystringify": "^2.0.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-moment": "^0.9.2",
		"react-navigation": "^3.9.1",
		"react-render-html": "^0.6.0",
		"stats-js": "^1.0.1",
		"three": "^0.100.0",
		"three-orbitcontrols": "^2.99.1"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/preset-env": "^7.4.3",
		"@babel/preset-react": "^7.0.0",
		"@wordpress/eslint-plugin": "^4.1.0",
		"@wordpress/prettier-config": "^0.1.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^25.3.0",
		"babel-loader": "^8.0.5",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^25.3.0",
		"path": "^0.12.7",
		"prettier": "npm:wp-prettier@^1.19.1",
		"stylelint": "^13.3.0",
		"stylelint-config-wordpress": "^16.0.0",
		"stylelint-webpack-plugin": "^1.2.3",
		"eslint": "^6.8.0",
		"eslint-loader": "^4.0.0",
		"eslint-plugin-eslint-comments": "^3.1.2",
		"eslint-plugin-jsdoc": "^22.1.0",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0",
		"webpack-dev-server": "^3.2.1",
		"node-sass": "^4.13.1",
		"react-test-renderer": "^16.13.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^0.23.1",
		"url-loader": "^1.1.2",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"image-webpack-loader": "^4.6.0"
	},
	"jest": {
		"testMatch": [
			"**/test/**/*.[jt]s?(x)"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
